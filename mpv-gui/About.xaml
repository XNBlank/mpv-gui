        <Label x:Name="label_Copy" Content="Created by XNBlank" HorizontalAlignment="Left" Margin="127,30,0,0" VerticalAlignment="Top" Foreground="White" FontSize="10" Width="107"/>
        <Label x:Name="label_Copy1" Content="v1.0" HorizontalAlignment="Left" Margin="183,15,0,0" VerticalAlignment="Top" Foreground="#FF707070" Background="#00000000" FontSize="10" Width="30"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="132,49,0,0" TextWrapping="Wrap" Text=">> " VerticalAlignment="Top" FontSize="10" Foreground="#FFCFCFCF" Width="59"><Hyperlink NavigateUri="https://github.com/xnblank/" RequestNavigate="Hyperlink_RequestNavigate" Foreground="#FFC5C5C5">Github</Hyperlink></TextBlock>

